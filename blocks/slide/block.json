{
    "name": "wpe/slide",
    "apiVersion": 2,
    "title": "Slide",
    "description": "Add slides to a slider",
    "category": "media",
    "icon": "format-image",
    "keywords": ["slider", "slideshow", "carousel"],
    "acf": {
        "mode": "preview",
        "renderTemplate": "template.php"
    },
    "attributes": {
        "slideImg": {
            "type": "object",
            "default": {
                "src": "https://acf-slider.local/wp-content/plugins/acf-slider-block/blocks/slider/assets/image1.jpg"
            }
        },
        "slideTitle": {
            "type": "string",
            "default": "Slide Title"
        }
    },
    "parent": [ "wpe/slider" ],
    "supports": {
		"align": false,
        "anchor": true,
        "className": true,
        "color": {
            "text": true,
            "background": true
        },
        "reusable": false
    }
}